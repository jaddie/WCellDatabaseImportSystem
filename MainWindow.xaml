        <Button Content="Exit Now" Height="23" HorizontalAlignment="Left" Margin="428,223,0,0" Name="BtnExit" VerticalAlignment="Top" Width="75" />
        <Button Content="Help" Height="23" HorizontalAlignment="Right" Margin="0,12,0,0" Name="BtnHelp" VerticalAlignment="Top" Width="75" />
        <Button Content="Settings" Height="23" HorizontalAlignment="Left" Margin="435,41,0,0" Name="BtnConfig" VerticalAlignment="Top" Width="75" Click="BtnConfig_Click" />
        <Button Content="Refresh" Height="23" HorizontalAlignment="Left" Margin="435,70,0,0" Name="BtnRefreshDBList" VerticalAlignment="Top" Width="75" Click="BtnRefreshDbListClick" />
        <TextBlock Height="97" HorizontalAlignment="Left" Margin="3,178,0,0" Name="TBoxOutput" Text="Verbose Output" VerticalAlignment="Top" Width="416" />
    </Grid>
</Window>
