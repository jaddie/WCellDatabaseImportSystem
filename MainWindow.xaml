<Window x:Class="WCellDatabaseImportSystem.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="WCell Database Importer" Height="321" Width="525" Name="WCellDatabaseImporter">
    <Grid Height="279">
        <ListBox Height="143" HorizontalAlignment="Left" Name="LBoxMainDBFiles" VerticalAlignment="Top" Width="205" />
        <Button Content="Import DB" Height="23" HorizontalAlignment="Left" Margin="46,149,0,0" Name="BtnApplyDBFile" VerticalAlignment="Top" Width="75" Click="BtnApplyDbFileClick" />
        <ListBox Height="143" HorizontalAlignment="Left" Name="LBoxDBUpdateFiles" VerticalAlignment="Top" Width="208" Margin="211,0,0,0" />
